{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    {
      "name": "@semantic-release/github",
      "config": {
        "assets": [
          {
            "path": "email-provider-links-*.tgz",
            "label": "NPM Package"
          }
        ]
      }
    }
  ],
  "preset": "conventionalcommits",
  "releaseRules": [
    {
      "type": "docs",
      "scope": "README",
      "release": "patch"
    },
    {
      "type": "refactor",
      "release": "patch"
    },
    {
      "type": "style",
      "release": "patch"
    }
  ]
}

