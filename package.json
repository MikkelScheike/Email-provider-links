{
  "name": "@mikkelscheike/email-provider-links",
  "version": "1.4.0",
  "description": "A TypeScript package that provides direct links to email providers based on email addresses to streamline login and password reset flows",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "providers/emailproviders.json",
    "!dist/**/*.map",
    "!dist/**/security-demo.*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "dev": "tsx src/index.ts",
    "sync-versions": "node scripts/sync-versions.js",
    "pretest": "node scripts/sync-versions.js",
    "prebuild": "node scripts/sync-versions.js"
  },
  "keywords": [
    "email",
    "provider",
    "login",
    "password-reset",
    "gmail",
    "outlook",
    "yahoo",
    "typescript",
    "npm",
    "utility"
  ],
  "author": "Mikkel Scheike",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikkelscheike/email-provider-links.git"
  },
  "homepage": "https://github.com/mikkelscheike/email-provider-links#readme",
  "bugs": {
    "url": "https://github.com/mikkelscheike/email-provider-links/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.11.1",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "conventional-changelog-conventionalcommits": "^9.0.0",
    "jest": "^30.0.1",
    "semantic-release": "^24.2.5",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
